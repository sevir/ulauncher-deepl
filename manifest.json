{
  "required_api_version": "^2.0.0",
  "name": "Deepl",
  "description": "Deepl extension",
  "developer_name": "Jose F Rives",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "deepl_kw",
      "type": "keyword",
      "name": "Deepl Keyword",
      "default_value": "dpl"
    },
    {
      "id": "deepl_apikey",
      "type": "input",
      "name": "Deepl APIkey",
      "description": "Register your apikey in https://www.deepl.com/pro?cta=header-pro-button",
      "default_value": ""
    }
  ]
}
